{
  "name": "jiggle",
  "author": "James Smith",
  "version": "0.0.214",
  "license": "MIT",
  "homepage": "https://github.com/djalbat/Jiggle",
  "description": "A combination of JSX and WebGL.",
  "repository": {
    "type": "git",
    "url": "https://github.com/djalbat/Jiggle"
  },
  "dependencies": {
    "express": "^4.15.4",
    "minimist": "^1.2.0",
    "necessary": "^1.9.6",
    "sharp": "^0.18.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browserify": "^13.0.0",
    "chokidar-cli": "^1.2.0",
    "gl-mat2": "^1.0.1",
    "gl-mat3": "^1.0.0",
    "gl-mat4": "^1.1.4",
    "gl-vec2": "^1.0.0",
    "gl-vec3": "^1.0.3",
    "gl-vec4": "^1.0.1",
    "necessary": "^1.7.7"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "babel": "babel ./es6 --presets=es2015,react --plugins transform-react-jsx --out-dir=./lib",
    "babel-debug": "babel ./es6 --presets=es2015,react --plugins transform-react-jsx --source-maps inline --out-dir=./lib",
    "browserify": "browserify ./lib/examples.js --standalone examples > ./public/script/examples.js",
    "browserify-debug": "browserify ./lib/examples.js --standalone examples --debug > ./public/script/examples.js",
    "build": "npm run clean && npm run babel && npm run browserify",
    "build-debug": "npm run babel-debug && npm run browserify-debug",
    "watch": "npm run build && chokidar './index.js' './es6/**/*.js' -c 'npm run build'",
    "watch-debug": "npm run build-debug && chokidar './index.js' './es6/**/*.js' -c 'npm run build-debug'"
  }
}
