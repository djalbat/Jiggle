{
  "name": "xgl",
  "author": "James Smith",
  "version": "1.5.39",
  "license": "MIT, Anti-996",
  "homepage": "https://github.com/djalbat/xgl",
  "description": "Makes use of JSX to leverage WebGL.",
  "repository": {
    "type": "git",
    "url": "https://github.com/djalbat/xgl"
  },
  "dependencies": {
    "express": "^4.16.4",
    "necessary": "^2.6.4",
    "xgl-server": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "browserify": "^16.5.0",
    "concurrently": "^5.1.0",
    "lively-cli": "^1.0.2",
    "watchful-cli": "^1.0.10"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "lively": "lively --watch-pattern=./public/lib/example.js --allowed-origin=http://localhost:8000 --quietly",
    "watchful": "watchful --source-directory=./es6 --temp-directory=./lib --entry-file=example.js --bundle-file=./public/lib/example.js",
    "batch": "npm run watchful batch --",
    "batch-debug": "npm run watchful batch -- --debug",
    "incremental": "npm run watchful incremental --",
    "incremental-debug": "npm run watchful incremental -- --debug",
    "build": "npm run clean && npm run batch",
    "build-debug": "npm run clean && npm run batch-debug",
    "watch": "npm run clean && npm run batch && concurrently 'npm run incremental' 'npm run lively'",
    "watch-debug": "npm run clean && npm run batch-debug && concurrently 'npm run incremental-debug' 'npm run lively'"
  }
}
